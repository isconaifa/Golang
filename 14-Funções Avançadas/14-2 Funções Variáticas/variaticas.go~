package main

import "fmt"

func soma(numeros ...int) int {
	total := 0
	for _, numero := range numeros {
		total += numero
	}
	return total
}

func main() {
	fmt.Println("Funções Variáticas")
	// Funções Variáticas funcionam de modo semelhante ao parametro rest do javascript
	calculo := soma(10, 20, 30)
	fmt.Println(calculo)
}
